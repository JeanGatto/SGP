<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
		<httpProtocol>
			<customHeaders>
				<remove name="Server" />
				<remove name="X-AspNetMvc-Version" />
				<remove name="X-AspNet-Version" />
				<remove name="X-Powered-By" />
				<remove name="X-Powered-By-Plesk" />
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>